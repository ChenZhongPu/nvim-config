{
  "pgfplots": {
    "prefix": "pgfplots",
    "body": [
      "\\documentclass{standalone}",
      "\\usepackage{pgfplots}",
      "\\pgfplotsset{width=10cm,compat=1.18}",
      "\\begin{document}",
      "\\begin{tikzpicture}",
      "\\begin{axis}",
      "${1}",
      "\\end{axis}",
      "\\end{tikzpicture}",
      "\\end{document}",
      "${0}"
    ],
    "description": "Creates a pgfplots standalone document"
  },
  "tikz": {
    "prefix": "tikz",
    "body": [
      "\\documentclass{standalone}",
      "\\usepackage{tikz}",
      "\\begin{document}",
      "\\begin{tikzpicture}",
      "${1}",
      "\\end{tikzpicture}",
      "\\end{document}",
      "${0}"
    ],
    "description": "Creates a standalone tikz document"
  },
  "documentCN": {
    "prefix": "documentcn",
    "body": [
      "\\documentclass{article}",
      "\\usepackage{xeCJK}",
      "\\usepackage{amsmath}",
      "\\title{${1:title}}",
      "\\author{${2:author}}",
      "\\date{\\today}",
      "\\begin{document}",
      "\\maketitle",
      "${3}",
      "\\end{document}",
      "${0}"
    ],
    "description": "Creates a Chinese document with math"
  }
}
